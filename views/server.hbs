{{#if loggedIn}}
<h1>{{title}}</h1>
<p>{{message}}</p>
Currently having {{countServer}} server
<br />
<table border="1">
	<tr>
		<th>IP</th>
		<th>UUID</th>
	</tr>
	{{#each listServer}}
		<tr>
			<td>{{this.ip_address}}</td>
			<td>{{this.uuid}}</td>
		</tr>
	{{/each}}
</table>
<br />
<form action="/server" method="post">
	<button type="submit" name="buy_server">Buy Server ($50000)</button>
</form>
{{else}}
You must be logged in to see this page!
{{/if}}
